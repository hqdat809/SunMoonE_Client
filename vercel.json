{
  "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }],
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "http://localhost:8080/api/v1/:path*"
    },
    {
      "source": "/kiot/:path*",
      "destination": "https://public.kiotapi.com/:path*"
    },
    {
      "source": "/token/:path*",
      "destination": "https://id.kiotviet.vn/connect/token/:path*"
    }
  ]
}
